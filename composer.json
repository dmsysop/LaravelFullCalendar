{
    "name": "dmsysop/laravelfullcalendar",
    "description": "Helper para Laravel utilizando FullCalendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Mariano",
            "email": "douglasm@outlook.com"
        }
